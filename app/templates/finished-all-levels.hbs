<div id="finished-all-levels">
    <div class="row">
        <div class="col-xs-12">
            <div class="page-header">
                <h1>Glückwunsch</h1>
            </div>
        </div>
    </div>


    <div class="row">
        <div class="col-xs-12">
            <p>
              {{t 'finishedAllLevels.text'}}
