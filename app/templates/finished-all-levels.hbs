            {{#if user.postTestUnlocked}}
              {{#unless user.postTestDone}}
                  <p>
                    {{t 'finishedAllLevels.postTest.1'}} {{#link-to 'posttest'}}Nachtest{{/link-to}} {{t 'finishedAllLevels.postTest.2'}}
                  </p>
                  <br/>
              {{/unless}}
            {{/if}}
            -->

